        "author": "@toulanboy",
        "repo": "https://github.com/toulanboy/scripts/blob/master/jd_price_detect",
        "icons": ["https://raw.githubusercontent.com/Orz-3/mini/master/jd.png", "https://raw.githubusercontent.com/Orz-3/task/master/jd.png"],
